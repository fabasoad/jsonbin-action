    "http",
    "mock",
    "rest",
    "json"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabasoad/jsonbin-action/issues"
  },
  "homepage": "https://github.com/fabasoad/jsonbin-action#readme",
  "dependencies": {
    "@actions/core": "1.9.1",
    "got": "12.3.1"
  },
  "devDependencies": {
    "@vercel/ncc": "0.34.0",
    "chai": "4.3.6",
    "dotenv": "16.0.1",
    "eslint": "8.22.0",
    "eslint-config-google": "0.14.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0"
  }
